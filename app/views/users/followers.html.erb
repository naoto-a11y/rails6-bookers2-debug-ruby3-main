<div class="container">
  <div class="row">
    <div class="col">
      <h1>Follower Users </h1>
      <table>
        <tr>
          <th><h3>image</h3></th>
          <th><h3>name</h3></th>
          <th></th>
          <th></th>
          <th></th>
        </tr>

        <% @users.each do |user| %>
        <tr>
          <td><%= image_tag user.get_profile_image, size:'100x100' %></td>
          <td><%= user.name %></td>
          <td>フォロー数：<%= user.followings.count %></td>
          <td>フォロワー数：<%= user.followers.count %></td>
          <td><%= link_to 'Show', user_path(user.id) %></td>
        <% end %>
        </tr>

      </table>
    </div>
  </div>
</div>